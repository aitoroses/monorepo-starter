{
  "name": "react-typescript-boilerplate",
  "version": "5.1.2",
  "description": "A seed project that uses typescript and babel together with react",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "./build/bundle.js",
  "bss": {
    "seedVersion": "5.1.2"
  },
  "scripts": {
    "patch": "tools/scripts/apply-patch",
    "start": "concurrently 'npm run dev' 'npm run mock-server' 'npm run proxy'",
    "test": "NODE_ENV=test karma start",
    "populatedb": "rm -rf test/mock-db/*.db && node test/populate-db.js",
    "dev": "node webpack/server.babel.js",
    "playground": "PLAYGROUND=true node webpack/playground.babel.js",
    "mock-server": "nodemon test/mock-server.js",
    "proxy": "devd -p 9000 -A 0.0.0.0 /app/=http://localhost:8080 /api/=http://localhost:7003 /static/=./static /workspace/=./workspace /playground/=http://localhost:6006",
    "tmux": "tools/scripts/tmux-dev-session",
    "scaffold": "react-seed-cli gen",
    "clean": "rm -rf build && rm -rf release",
    "prebuild": "CONTINUOUS_INTEGRATION=true npm test",
    "build": "npm run clean && webpack --config webpack/prod.babel.js",
    "release:default": "npm run clean && npm run build && mkdir -p release/static && cp -r static release && cp -r build release/lib && cp index.html release && ltc release/index.html && warbler release react-seed",
    "release:dev": "NODE_ENV=production API_URL=/api npm run release:default",
    "release": "NODE_ENV=production npm run release:default",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Grayfox",
  "license": "MIT",
  "devDependencies": {
    "@grayfox/react-seed-cli": "^0.2.0",
    "@kadira/storybook": "^2.32.1",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.0",
    "cssnano": "^3.8.0",
    "enzyme": "^2.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sinon-chai": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lolex": "^1.5.2",
    "long-term-cache": "^1.0.0",
    "mocha": "^3.0.2",
    "mocker-data-generator": "^1.2.2",
    "nocker": "^1.3.0",
    "node-http-proxy": "^0.2.3",
    "node-libs-browser": "^1.0.0",
    "nodemon": "^1.10.2",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "15.3.1",
    "request": "^2.79.0",
    "rucksack-css": "^0.8.5",
    "script-loader": "^0.7.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "toml": "^2.3.0",
    "ts-loader": "^1.2.2",
    "typescript": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-bundle-analyzer": "^1.5.3",
    "webpack-dev-server": "^1.15.1"
  },
  "dependencies": {
    "@grayfox/tsmonad": "^0.7.0",
    "angular2-di": "^2.0.0-rc.4",
    "bss-hw-api": "2.1.3",
    "bss-hw-state": "^1.0.0",
    "classnames": "^2.2.5",
    "glamor": "^2.19.0",
    "history": "^3.2.1",
    "immutable": "^3.8.1",
    "joi-browser": "^9.0.4",
    "lsbus": "^1.0.1",
    "material-ui": "^0.16.1",
    "moment": "^2.16.0",
    "nedb": "^1.8.0",
    "normalizr": "^2.2.1",
    "query-string": "^4.2.3",
    "ramda": "^0.22.1",
    "react": "15.3.1",
    "react-addons-perf": "^15.3.1",
    "react-art": "^0.15.1",
    "react-dom": "15.3.1",
    "react-motion": "^0.4.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.6",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-freeze": "^0.1.4",
